{% set col1 = 'col-bb5-43' %}
{% set col2 = 'col-bb5-14' %}
{% set col3 = 'col-bb5-14' %}
{% set col4 = 'col-bb5-14' %}
{% set col5 = 'col-bb5-6' %}
{% set col6 = 'col-bb5-6' %}
{% set col7 = 'col-bb5-3' %}
<div id="content-library" style="height: 100%;">
  <div id="content-library-pane-wrapper" class="bb5-windowpane-wrapper windowpane-treeview" style="height: 100%;">
    <div class="ui-layout-north">
      <div class="row">
        <div class="col-bb5-100">
          <div class="bb5-widget-toolbar list-options clearfix">
            <div id="bb-page-management-pagination-view"></div>
            <p id="bb-page-management-range-view" class="pull-right"></p>
          </div>
        </div>
      </div>
    </div><!--/.ui-layout-north-->

    <div class="ui-layout-pane ui-layout-west">
      <div id="bb-page-management-tree-view" class="bb5-windowpane-tree"></div>
      <div>
        <a id="bb-page-management-trash-view" href="#"><i class="fa fa-trash-o"></i> {{ 'trash'|trans }}</a>
      </div>
    </div>
    <div class="bb5-windowpane-main ui-layout-pane ui-layout-center">

      <div class="inner-center layout-overflow-scroll">
        <div class="bb5-list-data-toolbar bb5-toolbar-order">
          <div class="row">
            <p class="bb5-toolbar-order-col {{ col7 }}">
              <input id="bb-page-manage-select-all" type="checkbox">
            </p>
            <p class="bb5-toolbar-order-col {{ col1 }}">
              <a href="#" data-sorter="title" data-direction="asc">{{ 'title'|trans }} <i class="fa fa-angle-down"></i></a>
            </p>
            <p class="bb5-toolbar-order-col {{ col2 }}">
              <a href="#" data-sorter="state" data-direction="asc">{{ 'status'|trans }} <i class="fa fa-angle-down"></i></a>
            </p>
            <p class="bb5-toolbar-order-col {{ col3 }}">
              <a href="#" data-sorter="layout" data-direction="asc">{{ 'layout'|trans }} <i class="fa fa-angle-down"></i></a>
            </p>
            <p class="bb5-toolbar-order-col {{ col4 }}">
              <a href="#" data-sorter="modified" data-direction="desc">{{ 'updated'|trans }} <i class="fa fa-angle-down"></i></a>
            </p>
            <p class="bb5-toolbar-order-col {{ col6 }}">
              {{ 'remove'|trans }}
            </p>
          </div>
        </div><!--/.bb5-list-data-toolbar-->
        <div id="bb-page-management-data-view">

        </div><!--/.bb5-list-users : list -->
      </div><!--/.inner-center-->

      <div class="ui-layout-south">
        <p class="selection-toolbar">
          <i class="fa fa-level-up fa-rotate-90"></i> {{ 'apply_to_all_selected_pages'|trans }}
          <select id="bb-page-manage-grouped-action" class="max-per-page-selector input-xs">
            <option value="0">{{ 'choose_an_action'|trans }}</option>
            <option value="setOnlineGrouped">{{ 'set_online'|trans }}</option>
            <option value="setOfflineGrouped">{{ 'set_offline'|trans }}</option>
            <option value="removeGrouped">{{ 'remove'|trans }}</option>
            {#<option value="changeParentGrouped">{{ 'change_parent'|trans }}</option>#}
          </select>
        </p>
      </div><!--/.ui-layout-south-->

    </div>
  </div>
</div><!--id:content-library-->
