<form id="{{ form.getId() }}"
      role="form" method="{{ form.getMethod() }}"
      {% if form.getAction() != null %}action="{{ form.getAction() }}"{% endif %}
>
  {% for element in elements %}
      {{ element }}
  {% endfor %}
  <button type="submit" class="btn btn-default">{{ form.getSubmitLabel() }}</button>
</form>